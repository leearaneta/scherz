;; shadow-cljs configuration
{:source-paths
 ["src/"
  "test/"]

 :dependencies
 [[org.clojure/math.combinatorics "0.1.6"]]

 :builds {:scherz {:target :node-library
                   :output-to "out/scherz/lib.js"
                   :exports {:util scherz.exports/util
                             :generate scherz.exports/generate}
                   :compiler-options {:elide-asserts false}}
          :scherz.brightness {:target :node-library
                              :output-to "out/scherz.brightness/lib.js"
                              :exports-var scherz.exports/brightness
                              :compiler-options {:elide-asserts false}}}}

